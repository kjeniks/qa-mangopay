{
  "compilerOptions": {
    "noImplicitAny": false,
    "outDir": "./dist/",
    "baseUrl": "./",
    "paths": {
      "@pages/*": ["./cypress/pages/*"]
    },
    "module": "es6",
    "target": "es6",
    "allowJs": true,
    "types": ["cypress", "node"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["**/*.ts"]
}
